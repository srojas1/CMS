{
    "name": "mccool/laravel-auto-presenter",
    "description": "A system for auto-decorating models with presenter objects.",
    "keywords": ["lpm", "laravel", "eloquent", "presenter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shawn McCool",
            "email": "shawn@heybigname.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/container": "~5.0",
        "illuminate/contracts": "~5.0",
        "illuminate/events": "~5.0",
        "illuminate/pagination": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/view": "~5.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "McCool\\LaravelAutoPresenter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McCool\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
